                    render 'product-icon-label', icon: block.settings.icon_2_image, label: block.settings.icon_2_label, style: 'compact', layout: block.settings.labels_style
                    render 'product-icon-label', icon: block.settings.icon_3_image, label: block.settings.icon_3_label, style: 'compact', layout: block.settings.labels_style
                    render 'product-icon-label', icon: block.settings.icon_4_image, label: block.settings.icon_4_label, style: 'compact', layout: block.settings.labels_style
                    render 'product-icon-label', icon: block.settings.icon_5_image, label: block.settings.icon_5_label, style: 'compact', layout: block.settings.labels_style
                    render 'product-icon-label', icon: block.settings.icon_6_image, label: block.settings.icon_6_label, style: 'compact', layout: block.settings.labels_style
                    render 'product-icon-label', icon: block.settings.icon_7_image, label: block.settings.icon_7_label, style: 'compact', layout: block.settings.labels_style
                    render 'product-icon-label', icon: block.settings.icon_8_image, label: block.settings.icon_8_label, style: 'compact', layout: block.settings.labels_style
